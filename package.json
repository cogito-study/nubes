{
  "name": "graphql-auth",
  "license": "MIT",
  "scripts": {
    "start:dev": "env-cmd .env.dev ts-node src/index.ts",
    "start:staging": "env-cmd .env.staging ts-node src/index.ts",
    "start:prod": "env-cmd .env.prod ts-node src/index.ts",
    "prisma-generate:dev": "prisma generate --env-file .env.dev",
    "prisma-generate:staging": "prisma generate --env-file .env.staging",
    "prisma-generate:prod": "prisma generate --env-file .env.prod",
    "prisma-token:dev": "prisma token --env-file .env.dev",
    "prisma-token:staging": "prisma token --env-file .env.staging",
    "prisma-token:prod": "prisma token --env-file .env.prod",
    "prisma-deploy:dev": "prisma deploy --env-file .env.dev",
    "prisma-deploy:staging": "prisma deploy --env-file .env.staging",
    "prisma-deploy:prod": "prisma deploy --env-file .env.prod",
    "generate": "graphqlgen"
  },
  "dependencies": {
    "bcrypt": "3.0.3",
    "graphql-import": "^0.7.1",
    "graphql-shield": "5.1.0",
    "graphql-yoga": "1.16.9",
    "jsonwebtoken": "8.4.0",
    "prisma-client-lib": "1.23.4",
    "ts-node": "7.0.1",
    "env-cmd": "^8.0.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.17",
    "@types/bcrypt": "3.0.0",
    "@types/graphql": "14.0.4",
    "@types/jsonwebtoken": "8.3.0",
    "@types/ws": "6.0.1",
    "prettier": "1.15.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "3.2.2"
  }
}
